{
    "name": "NMGWorker",
    "version": "1.0",
    "description": "NMG Chrome-Extension Blank Template",
    "permissions": [
      "declarativeContent", 
      "storage"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "index.html"
    },
    "icons": {
      "16": "logo_16.png",
      "32": "logo_32.png",
      "48": "logo_48.png",
      "128": "logo_128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
  }